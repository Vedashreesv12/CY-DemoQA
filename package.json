{
  "name": "cy-demoqa",
  "version": "1.0.0",
  "description": "Using Cypress to test on demoqa.com",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "test:chrome": "cypress run --browser chrome",
    "test:chrome-headed": "cypress run --browser chrome --headed"

  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "html": {
      "enabled": true
    }
  },
  "format": "prettier --write '**/*.{js,ts,json,feature}'",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vedashreesv12/CY-DemoQA.git"
  },
  "keywords": [
    "Automation"
  ],
  "author": "Veda",
  "license": "MIT",
  "homepage": "https://github.com/Vedashreesv12/CY-DemoQA#readme",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.10.6",
    "cypress": "^13.17.0",
    "typescript": "^5.3.3"
  }
}
